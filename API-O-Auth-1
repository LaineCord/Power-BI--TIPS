#CRIAR A FUNÇÃO ABAIXO PARA ACESSO A API COM TOKEN

(Page as text) =>


let
cabecalho =
    [
        Authorization = "Bearer token aqui"
    ],

Fonte = Json.Document(
     Web.Contents(
                "Url da API aqui",
    [
        Headers =  cabecalho,
        RelativePath = "endpointAQUI?page="&Text.From(Page)
        
           
    ]
     ))
 
in
Fonte
